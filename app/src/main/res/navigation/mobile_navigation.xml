<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.emarketapplication.UserInterface.ui.home.HomeFragment"
        android:label="@string/home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.emarketapplication.UserInterface.ui.news.NewsFragment"
        android:label="@string/news"
        tools:layout="@layout/fragment_news" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.emarketapplication.UserInterface.ui.myCart.MyCartFragment"
        android:label="@string/mycart"
        tools:layout="@layout/fragment_mycart" />
    <fragment
        android:id="@+id/navigation_More"
        android:name="com.example.emarketapplication.UserInterface.ui.more.MoreFragment"
        android:label="More"
        tools:layout="@layout/fragment_more" />
</navigation>